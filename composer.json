  "minimum-stability": "beta",
  "license": "MIT",
  "authors": [
    {
      "name": "Julio Rabadán González",
      "email": "julio@somms.net"
    }
  ],
  "require": {
    "php": ">=8.2",
    "duzun/hquery": "~2.2.0",
    "ext-json": "*"
